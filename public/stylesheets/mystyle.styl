.facebook, .twitter, .google, .linkedin
  width 100%
  height 40px
  padding-top 2%
  margin-top 2%

.google, .google:hover
  background-color #dd4b39
  border 2px solid #dd4b39
  color #fff

.twitter, .twitter:hover
  background-color #00aced
  border 2px solid #00aced
  color #fff

.facebook, .facebook:hover
  background-color #3b5999
  border 2px solid #3b5999
  color #fff

.linkedin, .linkedin:hover
  background-color #007bb6
  border 2px solid #007bb6
  color #fff

html
  position relative
  min-height 100%

body
  padding-top 20px

.carosal
  padding-top 20px

.footer
  position relative
  width 100%
  height 200px
  background-color #f5f5f5
  text-align center

h3
  text-align center

.info
  padding-top 20px
  padding-bottom 20px

#map-canvas
  width 100%
  height 300px

.canvas
  padding-top 20px

.glyphicon-map-marker
  font-size 20px

.text-muted
  margin 20px 0

.login_modal_footer
  margin-top 5px
  background #29A629
  color #fff

.form-group
  position relative
  .login-field-icon
    font-size 20px
    position absolute
    right 15px
    top 3px
    transition all 0.25s ease 0s
    padding-top 2%

.login-modal
  width 100%
  padding-bottom 20px
  input
    height 40px
    box-shadow none
    border 1px solid #ddd

.modal-register-btn
  margin 4% 33% 2%
  width 100%

.modal-body-left
  float left
  width 50%
  padding-right 4%
  border-right 4px solid #ddd

.modal-body-right
  float right
  width 47%

.login-link
  padding 0 20%

.modal-social-icons
  padding 0 10%

.modal-icons
  margin-left -10px
  margin-right 20px

#social-icons-conatainer
  position relative

#center-line
  position absolute
  right 265.7px
  top 80px
  background #ddd
  border 4px solid #DDDDDD
  border-radius 20px

.modal-login-btn
  width 100%
  height 40px
  margin-bottom 10px
  border none

#modal-launcher
  margin 30% 0 0 30%

h1
  border-bottom 5px solid #00BB64

.navbar-default
  border none
  .navbar-header
    .navbar-brand
      color #fff
      font-size 30px
      font-family Schoolbell, cursive
      padding-top 8px
      & > i
        color #2EB82E
  .navbar-nav
    & > li
      & > a
        &:hover
          background #2EB82E
          font-size 25px

.infoabout
  margin-top 5px
  margin-bottom 13px
  .row
    .col-md-4
      & > i
        font-size 130px
        margin-left 33%
        color #222222

.login_modal_header
  background #29A629
  .modal-title
    color #fff
    text-align center

a
  &:active
    background #2EB82E
    

.form-inline
  margin-left 18%
  margin-right 5%

.searchres
  .col-md-8
    & > a
      & > h2
        &:hover
          font-size 33px;